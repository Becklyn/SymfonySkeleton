{
    "name": "becklyn/symfony-skeleton",
    "type": "project",
    "license": "BSD-3-Clause",
    "description": "Symfony skeleton for Becklyn projects",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "becklyn/yaml-parameters": "dev-test#3564058dd5683db5912068e01d77811ccebf353c",
        "symfony/console": "^4.0",
        "symfony/flex": "dev-interactivity",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": [
        ],
        "parameters": "Becklyn\\YamlParameters\\Script::handle",
        "post-install-cmd": [
            "@parameters",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@parameters",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "",
            "allow-contrib": false
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/apfelbox/flex"
        }
    ]
}
